<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CustomButton" parent="Theme.Material3.Dark.NoActionBar">
        <item name="backgroundTint">@color/primary40</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">sans-serif-light</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">20sp</item>
        <item name="cornerRadius">20dp</item>
    </style>

    <style name="DialogTheme" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:colorPrimaryDark">@color/primary80</item>
        <item name="android:windowBackground">@color/primary95</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowIsFloating">false</item>
    </style>
</resources>